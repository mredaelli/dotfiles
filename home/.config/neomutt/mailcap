# text/html; w3m -dump -s -o display_link=yes -o display_link_number=yes -o decode_url=yes -T text/html -I %{charset}; copiousoutput; description=HTML Text

image/*;  t=$(mktemp /tmp/XXXXXXXXXX) \; cp %s "$t" \; /bin/sh -c "sxiv -. $t \; rm $t \;" &; test=test -n "$DISPLAY"

text/html; t=$(mktemp /tmp/mutt.XXXXXXXXXX) \; cp %s "$t" \; firefox "$t" &; description=HTML Text; test=test -n "$DISPLAY";  nametemplate=%s.html

text/html;\
	w3m -T text/html -I %{charset} -dump %s;\
	nametemplate=%s.html;\
	copiousoutput

application/vnd.ms-word; soffice '%s'

application/pdf; zathura '%s'

text/calendar; khal import -a personal '%s'
application/ics; khal import -a personal '%s'
